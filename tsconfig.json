{
	"compilerOptions": {
		"target": "ES5",
		"module": "ESNext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx",
		"noImplicitAny": false,
		// "declaration": true,
		// "declarationMap": true, 
		// "declarationDir": "typess",
		"outDir": "dist",
		"rootDir": "src"
	},
	"include": ["src", "src/callRender.tsx"]
}
